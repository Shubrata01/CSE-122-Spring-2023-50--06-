//Palindrome code
//Lab 02  16/1/2023


#include<iostream>
using namespace std;

int main(){
int n,r,sum=0,rev;
cout<<"Enter the palindrome number \n";
cin>>n;
rev=n;
while(n>0)
{
    r=n%10;
    sum=(sum*10)+r;
    n=n/10;
}
if (rev==sum)
    cout<<"Number is palindrome";
else
    cout<<"Number is not palindrome";
return 0;
}
